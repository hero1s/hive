--工程名字
PROJECT_NAME = "luna"

--目标名字
TARGET_NAME = "luna"

--工程类型: static/dynamic/exe
PROJECT_TYPE = "static"

--需要的FLAGS
FLAGS = {
	"Wno-unused-variable",
	"Wno-unused-parameter",
	"Wno-unused-but-set-parameter",
    "Wno-unused-function",
    "Wno-sign-compare",
}

--需要的include目录
INCLUDES = {
	"../../extend/lua/lua"
}

--目标文件前缀
LIB_PREFIX = 1

--依赖项目
DEPS = {
	"lualib"
}

--分组定义
GROUP = "core"
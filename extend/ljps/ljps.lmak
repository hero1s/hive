--是否生效
ENABLE = false

--工程名字
PROJECT_NAME = "ljps"

--目标名字
TARGET_NAME = "ljps"

----工程类型: static/dynamic/exe
PROJECT_TYPE = "dynamic"

--需要的FLAGS
FLAGS = {
	"Wno-sign-compare"
}

--需要定义的选项
DEFINES = {
}

--需要的include目录
INCLUDES = {
	"../lua/lua",
	"../luakit/include",
}

--需要连接的库文件
LIBS = {
	"lua"
}

--WINDOWS需要定义的选项
WINDOWS_DEFINES = {
	"LUA_BUILD_AS_DLL"
}

--依赖项目
DEPS = {
	"lualib"
}

--分组定义
GROUP = "luaext"
